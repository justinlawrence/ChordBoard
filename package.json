{
	"name": "chordboard",
	"version": "1.0.0",
	"private": true,
	"description": "An open-source chord sheet manager for live musicians.",
	"config": {
		"port": 8080
	},
	"scripts": {
		"api": "node src/server",
		"clean": "rimraf ./build/",
		"dev-server": "webpack-dev-server --env=dev --inline --hot",
		"start": "npm run dev-server",
		"test": "jest",
		"deploy": "npm run clean && npm run build && rsync -azP --delete ./build/ irjweb:/var/www/chordboard",
		"deploy:beta": "npm run clean && npm run build && rsync -azP --delete ./build/ irjweb:/var/www/chordboard-beta",
		"deploy-cloudnode": "npm run build && rsync -azP --exclude=.git --delete ./build/ /var/www/chordboard-cloudnode && git -C /var/www/chordboard-cloudnode add . && git -C /var/www/chordboard-cloudnode commit -m \"auto-build\" && git -C /var/www/chordboard-cloudnode push origin master",
		"build": "webpack -p --env=prod --progress"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/justinlawrence/chordboard.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/justinlawrence/chordboard/issues"
	},
	"homepage": "https://github.com/justinlawrence/chordboard#readme",
	"dependencies": {
		"@material-ui/core": "3.5.1",
		"@sentry/browser": "^4.3.2",
		"babel-regenerator-runtime": "^6.5.0",
		"chord-magic": "^2.1.1",
		"copy-webpack-plugin": "^4.0.1",
		"date-fns": "^1.29.0",
		"file-loader": "^2.0.0",
		"lodash": "4.17.11",
		"mdi-material-ui": "5.6.0",
		"pouchdb": "7.0.0",
		"pouchdb-find": "7.0.0",
		"prop-types": "15.6.2",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"react-facebook-login": "^4.1.1",
		"react-google-login": "^3.2.1",
		"react-redux": "^5.1.1",
		"react-router-dom": "4.3.1",
		"react-textarea-autosize": "^7.1.0-1",
		"redux": "4.0.1",
		"redux-actions": "^2.6.4",
		"redux-devtools-extension": "2.13.5",
		"redux-pouchdb": "0.1.4",
		"redux-saga": "0.16.2",
		"rimraf": "^2.6.2",
		"slugify": "^1.2.1",
		"url": "latest",
		"url-loader": "^1.1.2"
	},
	"devDependencies": {
		"autoprefixer": "^9.3.1",
		"babel-core": "^6.26.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^7",
		"babel-preset-env": "^1.6.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"css-loader": "^1.0.1",
		"eslint": "^5.9.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-prettier": "^3.0.0",
		"eslint-plugin-react": "^7.11.1",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"html-webpack-plugin": "^3.2.0",
		"img-loader": "^3.0.1",
		"jest": "^23.1.0",
		"node-sass": "^4.9.0",
		"offline-plugin": "5.0.5",
		"postcss-loader": "^3.0.0",
		"prettier": "^1.15.3",
		"raw-loader": "^0.5.1",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.23.1",
		"webpack": "^4.25.1",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^ 3.1.10",
		"webpack-merge": "^4.1.0"
	}
}
