{
  "name": "chordboard",
  "version": "1.0.0",
  "private": true,
  "description": "A decentralised, open-source chord sheet manager for live musicians.",
  "config": {
    "port": 8080
  },
  "scripts": {
    "dev-server": "webpack-dev-server --env=dev --progress --hot --inline",
    "start": "npm run dev-server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && scp -rp build irjweb:/var/www/chordboard",
    "build": "webpack -p --env=prod --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlawrence/chordboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justinlawrence/chordboard/issues"
  },
  "homepage": "https://github.com/justinlawrence/chordboard#readme",
  "dependencies": {
    "browser-sync": "^2.18.8",
    "preact": "7.1.0",
    "preact-router": "2.5.2",
    "slugify": "1.1.0",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-react-jsx": "6.22.0",
    "babel-preset-env": "1.3.3",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "html-webpack-plugin": "2.28.0",
    "img-loader": "1.3.1",
    "node-sass": "4.5.0",
    "postcss-loader": "1.1.1",
    "raw-loader": "0.5.1",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-merge": "4.1.0"
  }
}
