{
  "name": "chordboard",
  "version": "1.0.0",
  "private": true,
  "description": "An open-source chord sheet manager for live musicians.",
  "config": {
    "port": 8080
  },
  "scripts": {
    "api": "node src/server",
    "clean": "rimraf ./build/",
    "dev-server": "webpack-dev-server --env=dev --inline --hot",
    "start": "npm run dev-server",
    "test": "jest",
    "deploy": "npm run clean && npm run build && rsync -azP --delete ./build/ irjweb:/var/www/chordboard",
    "deploy:beta": "npm run clean && npm run build && rsync -azP --delete ./build/ irjweb:/var/www/chordboard-beta",
    "deploy-cloudnode": "npm run build && rsync -azP --exclude=.git --delete ./build/ /var/www/chordboard-cloudnode && git -C /var/www/chordboard-cloudnode add . && git -C /var/www/chordboard-cloudnode commit -m \"auto-build\" && git -C /var/www/chordboard-cloudnode push origin master",
    "build": "webpack -p --env=prod --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justinlawrence/chordboard.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justinlawrence/chordboard/issues"
  },
  "homepage": "https://github.com/justinlawrence/chordboard#readme",
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "babel-regenerator-runtime": "^6.5.0",
    "chord-magic": "^2.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "date-fns": "^1.29.0",
    "lodash": "4.17.10",
    "mdi-material-ui": "^5.1.1",
    "pouchdb": "7.0.0",
    "pouchdb-find": "7.0.0",
    "prop-types": "15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-facebook-login": "^4.0.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-pouchdb": "0.1.1",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.2",
    "slugify": "^1.2.1",
    "url": "latest"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "fauxton": "^1.1.13",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "offline-plugin": "5.0.5",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  }
}
